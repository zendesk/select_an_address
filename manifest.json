{
  "name": "Select Support Address",
  "author": {
    "name": "Zendesk Labs",
    "email": "zendesklabs@zendesk.com"
  },
  "defaultLocale": "en",
  "private": false,
  "location": [
    "new_ticket_sidebar",
    "ticket_sidebar"
  ],
  "version": "1.1.1",
  "frameworkVersion": "1.0",
  "parameters": [
    {
      "name": "force_selection_of_brand",
      "type": "checkbox"
    },
    {
      "name": "mandatory_requester_email",
      "type": "checkbox",
      "default": false
    },
    {
      "name": "new_tickets?",
      "type": "checkbox",
      "default": true
    },
    {
      "name": "existing_tickets?",
      "type": "checkbox",
      "default": true
    },
    {
      "name": "brand_field_id",
      "type": "number",
      "required": false
    },
    {
      "name": "brand_mapping",
      "type": "multiline",
      "required": false
    }
  ]
}
